isbn = {0171-8630},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Laboratory growth rates,Maximum growth,Moult frequency,Moult increments,Moult interval},
pages = {141--155},
title = {{Growth in the brown shrimp Crangon crangon. I. Effects of food, temperature, size, gender, moulting, and cohort}},
volume = {435},
year = {2011}
}
