@book{Kerr2013,
abstract = {Simulation models are useful and flexible tools that can be used to synthesize information gained from multiple stock identification methods. The assimilation of information from many sources into a population dynamics model can provide a holistic view of stock structure. Application of spatially explicit models within the simulation framework allows the testing of varying hypotheses regarding stock structure and connectivity. Additionally, using this approach we can examine the ecological, assessment, and management implications of complex spatial structure for the fish resource. This chapter summarizes the simulation modeling framework and how it can be applied to investigate population structure. We highlight how stock identification data can be incorporated into simulations and review recent advances in spatial modeling techniques. We conclude with a discussion of seminal case studies, which illustrate how simulation models have impacted our understanding of stock structure in fish populations. Ultimately, simulation models have many limitations because they are simplified abstractions of large and complex natural systems. However, when carefully vetted and used with strict adherence to the scientific method, simulations can be powerful and invaluable tools for examining spatial population structure. ?? 2014 Elsevier Inc. All rights reserved.},
author = {Kerr, Lisa A. and Goethel, Daniel R.},
booktitle = {Stock Identification Methods: Applications in Fishery Science: Second Edition},
