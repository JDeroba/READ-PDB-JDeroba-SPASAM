issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {July},
pages = {1--15},
title = {{Spatial modeling of Bering Sea walleye pollock with integrated age-structured assessment models in a changing environment}},
volume = {15},
year = {2013}
}
