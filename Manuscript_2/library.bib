issn = {02727714},
journal = {Estuarine, Coastal and Shelf Science},
keywords = {King George whiting,Length distribution,Nursery habitats,Port Phillip Bay,Stereo-video,Zostera nigricaulis},
pages = {291--299},
publisher = {Elsevier Ltd},
title = {{Seagrass edge effects on fish assemblages in deep and shallow habitats}},
url = {http://dx.doi.org/10.1016/j.ecss.2012.09.013},
