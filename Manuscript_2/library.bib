file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kasim, Mukai - 2006 - Contribution of benthic and epiphytic diatoms to clam and oyster production in the Akkeshi-ko estuary.pdf:pdf},
isbn = {0916-8370},
issn = {09168370},
journal = {Journal of Oceanography},
keywords = {Benthic diatoms,Clam,Epiphytic diatoms,Oyster,Pelagic diatoms,Resuspension,The Akkeshi-ko estuary},
number = {3},
pages = {267--281},
title = {{Contribution of benthic and epiphytic diatoms to clam and oyster production in the Akkeshi-ko estuary}},
volume = {62},
