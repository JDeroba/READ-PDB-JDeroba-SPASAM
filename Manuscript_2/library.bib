keywords = {Animals,Bivalvia,Bivalvia: metabolism,Carbon Dioxide,Carbon Dioxide: analysis,Carbon Dioxide: metabolism,Ecosystem,Oceans and Seas,Seawater,Seawater: chemistry},
pages = {221--47},
pmid = {23987912},
title = {{Ocean acidification in the coastal zone from an organism's perspective: multiple system parameters, frequency domains, and habitats.}},
