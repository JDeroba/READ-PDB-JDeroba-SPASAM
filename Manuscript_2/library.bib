journal = {Estuarine, Coastal and Shelf Science},
keywords = {Southern China,accumulation rate of sediments,carbon isotope ratio,organic carbon,sediment sources,the Pearl River Estuary},
number = {2},
pages = {190--196},
publisher = {Elsevier Ltd},
title = {{The sources and accumulation rate of sedimentary organic matter in the Pearl River Estuary and adjacent coastal area, Southern China}},
