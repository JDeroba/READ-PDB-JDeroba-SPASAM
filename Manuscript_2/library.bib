@article{Allain2011,
abstract = {リポフスチン・レベルは、この種のためのツールを決定している年齢としてリポフスチン定量化の適用性を評価するために、ズワイガニ（Chionoecetes opilio）で調査された。 脳の嗅脳細胞塊（OLCM）の中のリポフスチン顆粒の局在確認と定量化は、共焦顕微鏡と画像解析によって実施された。 大多数の未熟なカニは、検出可能なリポフスチンを持たなかった。 汚い甲羅状態（より大きい年齢の目安）をもつカニは、きれいな甲羅状態をもつカニより多くのリポフスチン（顆粒密度、エリア分率と平均顆粒サイズに関して）を持った; しかし、リポフスチンの量は、より温暖な種のそれより非常に低かった。 リポフスチン含量は、汚い甲羅のカニの間ででなくきれいな甲羅のカニの間で甲羅サイズと明らかに相関していた。 相関関係は、平均リポフスチン顆粒蛍光強度と甲羅幅の間で観察されなかった。 OLCMニューロン特徴も、調査された。 この密度還元がニューロン（構造の寸法の、または、両方とも増加）の喪失によるならばそれが知られていないが、逆の関係はニューロン密度と甲羅幅の間で観察された。 さらにまた、ポジティブな関係は、ニューロン・サイズと甲羅幅の間で観察された。 ニューロン・サイズの領域もより大きなカニで増加した。そして、OLCMで有糸分裂が活発な細胞の存在を示唆した。 リポフスチン・レベルが中で甲羅カニをきれいにして、サイズ（成熟（きれいに甲羅ズワイガニ）で）で増加したより汚い甲羅状態（おそらくより年をとった）カニで高かったが、最大の未熟なカニの検出可能なリポフスチンの欠如、成熟したカニのリポフスチンの一般に低いレベルとより年をとった（汚い甲羅）カニのサイズによる増加の不足はこの種のためのこの年齢推定ツールの適用性を制限する。},
author = {Allain, R. N. and Moriyasu, M. and Crawford, B. D. and Courtenay, S. C.},
doi = {10.1163/156854011X605729},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Allain et al. - 2011 - Lipofuscin Quantification as a Potential Tool for Age Estimation in Snow Crabs,IChionoecetes opilioI (O. Fabriciu.pdf:pdf},
issn = {0011216X},
journal = {Crustaceana},
number = {12},
pages = {1441--1463},
title = {{Lipofuscin Quantification as a Potential Tool for Age Estimation in Snow Crabs,{\textless}I{\textgreater}Chionoecetes opilio{\textless}/I{\textgreater} (O. Fabricius, 1788) (Decapoda, Oregoniidae)}},
volume = {84},
