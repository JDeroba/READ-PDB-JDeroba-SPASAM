@article{Walters2002,
abstract = {Much of the machinery of modem fish stock assessment is aimed at providing$\backslash$nbetter estimates of current stock size, but the adaptive and feedback$\backslash$npolicies needed for sustainable long-term management are specified$\backslash$nin terms of fishing mortality rate goals in relation to relative$\backslash$nstock size, and estimates of optimum fishing mortality rates have$\backslash$ndropped sharply since 1990. Should we elect to refocus assessment$\backslash$nmethods and data gathering more directly on estimation of current$\backslash$nand optimum fishing mortality rates, we could probably bypass many$\backslash$nof the costs and pitfalls that have plagued traditional assessment$\backslash$napproaches. Further, we could concentrate more on providing advice$\backslash$nabout how to implement safe feedback policies, rather than just contributing$\backslash$nto debates about how much is out there to be harvested.},
