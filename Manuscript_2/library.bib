issn = {01657836},
journal = {Fisheries Research},
keywords = {Natural mortality,Pacific Coast groundfish stocks,Simulation analyses,Stock Synthesis,Stock assessment},
number = {1},
pages = {89--94},
publisher = {Elsevier B.V.},
title = {{Estimating natural mortality within a fisheries stock assessment model: An evaluation using simulation analysis based on twelve stock assessments}},
url = {http://dx.doi.org/10.1016/j.fishres.2011.01.021},
volume = {109},
year = {2011}
}
