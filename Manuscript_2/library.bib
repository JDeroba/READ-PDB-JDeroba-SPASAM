pages = {408--414},
title = {{Interspecific and intraspecific variation of d13C and d15N in deposit- and suspension-feeding bivalves (Macoma balthica and Cerastoderma edule): Evidence of ontogenetic changes in feeding mode of Macoma balthica}},
volume = {49},
