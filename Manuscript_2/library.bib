file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Heard et al. - 2007 - A guide to the Thalassinidea (Crustacea Malacostraca Decapoda) oof the South Atlantic Bight.pdf:pdf},
journal = {NOAA Professional Paper NMFS},
number = {October},
