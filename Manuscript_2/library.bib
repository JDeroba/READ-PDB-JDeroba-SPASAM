keywords = {Hierarchical sampling,Recruitment,Semibalanus balanoides,Settlement,Spatial variation,Temporal variation},
month = {jan},
number = {2},
pages = {209--225},
title = {{Spatial and temporal variation in settlement and recruitment of the intertidal barnacle Semibalanus balanoides (L.) (Crustacea: Cirripedia) over a European scale}},
