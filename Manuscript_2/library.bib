journal = {Acta Adriatica},
keywords = {growth,length-weight relationship,mortality,population,upogebia pusilla,yield},
number = {1},
pages = {87--103},
title = {{Growth , mortality and yield of the mudprawn Upogebia pusilla (Petagna , 1792) (Crustacea : Decapoda : Gebiidea) from western Greece}},
