@article{McGilliard2015,
abstract = {Some fish stock assessments are conducted in regions that contain no-take marine reserves (NTMRs). NTMRs are$\backslash$r$\backslash$nexpected to lead to spatial heterogeneity in fish biomass by allowing a buildup of biomass inside their borders while fishing$\backslash$r$\backslash$npressure occurs outside. Stock assessments do not typically account for spatial heterogeneity caused by NTMRs, which may lead$\backslash$r$\backslash$nto biased estimates of biomass. Simulation modeling is used to analyze the ability of several stock assessment configurations to$\backslash$r$\backslash$nestimate current biomass after the implementation of a single, large NTMR. Age-structured spatial operating models with three$\backslash$r$\backslash$npatterns of ontogenetic movement are used to represent the “true” population dynamics. Results show that assessing populations$\backslash$r$\backslash$nas a single stock with use of fishery catch-rate data and without accounting for the NTMR results in severe underestimation$\backslash$r$\backslash$nof biomass for two of the movement patterns. Omitting fishery catch-rate data or allowing time-varying dome-shaped selectivity$\backslash$r$\backslash$nafter NTMR implementation leads to improved estimates of current biomass, but severe bias in estimated trends in biomass over$\backslash$r$\backslash$ntime. Performing separate assessments for fished areas and NTMRs leads to improved estimation performance in the absence of$\backslash$r$\backslash$nmovement among assessment areas, but can severely overestimate biomass otherwise. Performing a spatial assessment with$\backslash$r$\backslash$nestimation of movement parameters among areas was found to be the best way to assess a species, even when movement$\backslash$r$\backslash$npatterns were unknown. However, future work should explore the performance of spatial assessments when catchability varies$\backslash$r$\backslash$namong areas.},
author = {McGilliard, Carey R. and Punt, Andr{\'{e}} E. and Methot, Richard D. and Hilborn, Ray and Jacobson, Larry},
doi = {10.1139/cjfas-2013-0364},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McGilliard et al. - 2015 - Accounting for marine reserves using spatial stock assessments.pdf:pdf},
issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {September 2014},
pages = {262 -- 280},
title = {{Accounting for marine reserves using spatial stock assessments}},
volume = {72},
year = {2015}
}
@article{Sheldon2002,
abstract = {Abstract  The residence and flushing times of an estuary are two different concepts that are often confused. Flushing time is the time required for the freshwater inflow to equal the amount of freshwater originally present in the estuary. It is specific to freshwater (or materials dissolved in it) and represents the transit time through the entire system (e.g., from head of tide to the mouth). Residence time is the average time particles take to escape the estuary. It can be calculated for any type of material and will vary depending on the starting location of the material. In the literature, the term residence time is often used to refer to the average freshwater transit time and is calculated as such. Freshwater transit time is a more precise term for a type of residence time (that of freshwater, starting from the head of the estuary), whereas residence time is a more general term that must be clarified by specifying the material and starting distribution. We explored these two mixing time scales in the context of the Altmaha River estuary, Georgia, and present a comparison of techniques for their calculation (fraction of freshwater models and variations of box models). Segmented tidal prism models, another common approach, have data requirements similar to other models but can be cumbersome to implement properly. Freshwater transit time estimates from simple steady-state box models were virtually, identical to flushing times for four river-flow cases, as long as boxes were scaled appropriately to river flow, and residence time estimates from different box models were also in good agreement. Mixing time estimates from box models, were incorrect when boxes were imporperly scaled. Mixing time scales vary nonlinearly with river flow, so characterizing the range as well as the mean or median is important for a thorough understanding of the potential for within-estuary processing. We are now developing an imporved box model that will allow the calculation of a variety of mixing time scales using simulations with daily variable river discharge.},
author = {Sheldon, Joan E. and Alber, Merryl},
doi = {10.1007/BF02692226},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sheldon, Alber - 2002 - A comparison of residence time calculations using simple compartment models of the Altamaha River estuary, Georg.pdf:pdf},
isbn = {0160-8347},
issn = {0160-8347},
journal = {Estuaries},
number = {6},
pages = {1304--1317},
title = {{A comparison of residence time calculations using simple compartment models of the Altamaha River estuary, Georgia}},
volume = {25},
year = {2002}
}
