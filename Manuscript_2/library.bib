isbn = {0706-652X},
issn = {0706652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {4},
pages = {925--936},
title = {{Use of lipofuscin for resolving cohorts of western rock lobster ({\textless}i{\textgreater}Panulirus cygnus{\textless}/i{\textgreater})}},
volume = {55},
year = {1998}
