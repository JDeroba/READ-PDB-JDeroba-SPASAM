@article{Hicks2011,
abstract = {Understanding the effects of anthropogenically-driven changes in global temperature, atmospheric carbon dioxide and biodiversity on the functionality of marine ecosystems is crucial for predicting and managing the associated impacts. Coastal ecosystems are important sources of carbon (primary production) to shelf waters and play a vital role in global nutrient cycling. These systems are especially vulnerable to the effects of human activities and will be the first areas impacted by rising sea levels. Within these coastal ecosystems, microalgal assemblages (microphytobenthos: MPB) are vital for autochthonous carbon fixation. The level of in situ production by MPB mediates the net carbon cycling of transitional ecosystems between net heterotrophic or autotrophic metabolism. In this study, we examine the interactive effects of elevated atmospheric CO(2) concentrations (370, 600, and 1000 ppmv), temperature (6°C, 12°C, and 18°C) and invertebrate biodiversity on MPB biomass in experimental systems. We assembled communities of three common grazing invertebrates (Hydrobia ulvae, Corophium volutator and Hediste diversicolor) in monoculture and in all possible multispecies combinations. This experimental design specifically addresses interactions between the selected climate change variables and any ecological consequences caused by changes in species composition or richness.},
author = {Hicks, Natalie and Bulling, Mark T and Solan, Martin and Raffaelli, Dave and White, Piran C L and Paterson, David M},
doi = {10.1186/1472-6785-11-7},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hicks et al. - 2011 - Impact of biodiversity-climate futures on primary production and metabolism in a model benthic estuarine system.pdf:pdf},
isbn = {1472-6785},
issn = {1472-6785},
journal = {BMC ecology},
number = {1},
pages = {7},
pmid = {21320339},
publisher = {BioMed Central Ltd},
title = {{Impact of biodiversity-climate futures on primary production and metabolism in a model benthic estuarine system.}},
url = {http://www.biomedcentral.com/1472-6785/11/7},
volume = {11},
year = {2011}
}
