doi = {10.1111/2041-210X.12146},
editor = {Ramula, Satu},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Merow et al. - 2014 - Advancing population ecology with integral projection models a practical guide.pdf:pdf;:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Merow et al. - 2014 - Advancing population ecology with integral projection models a practical guide(2).pdf:pdf;:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Merow et al. - 2014 - Advancing population ecology with integral projection models A practical guide(3).pdf:pdf},
