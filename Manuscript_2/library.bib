isbn = {0887-3593},
issn = {0887-3593},
journal = {Journal of the North American Benthological Society},
keywords = {15 n,bases of stream food,food webs,ic matter as the,nitrogen,stable isotopes,streams,the relative roles of,tracer experiments,various sources of organ-,webs are},
number = {3},
pages = {429--448},
