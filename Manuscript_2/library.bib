isbn = {0008-4301},
journal = {Canadian Journal of Zoology 70(11):2275-2280. 1992.},
number = {June 1990},
pages = {6},
title = {{Gray whale foraging on ghost shrimp (Callianassa californiensis) in littoral sand flats of Puget Sound, USA. (Eschrichtius robustus)}},
year = {1992}
