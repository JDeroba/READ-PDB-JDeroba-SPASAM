issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Demography,Inverse methods,Models,Mortality,Population dynamics,Zooplankton},
pages = {4--10},
publisher = {Elsevier B.V.},
title = {{Estimation of mortality for stage-structured zooplankton populations: What is to be done?}},
url = {http://dx.doi.org/10.1016/j.jmarsys.2011.05.008},
volume = {93},
year = {2012}
}
