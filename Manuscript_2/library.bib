file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kentula, DeWitt - 2003 - Abundance of seagrass (Zostera marina L.) and macroalgae in relation to the salinity-temperature gradient in(2).pdf:pdf},
issn = {0160-8347},
journal = {Estuaries},
number = {4},
pages = {1130--1141},
title = {{Abundance of seagrass (Zostera marina L.) and macroalgae in relation to the salinity-temperature gradient in Yaquina Bay, Oregon, USA}},
