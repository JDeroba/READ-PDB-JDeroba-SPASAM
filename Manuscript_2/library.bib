@article{Miller1984,
abstract = {Three species of the estuarine amphipod genus Corophium employed oscillatory motions of the maxillipeds and maxillipedal palps to ingest selected particles and pass rejected particles anteriorly from the buccal mass. These species demonstrated strong, mechanical, post-capture selection for smaller particles over the size range of 5-53 @cm (diameter) in both suspension- and surface-deposit-feeding modes. The logarithm of particle probability of ingestion was linearly related to particle diameter. Selectivity was unaffected by the presence of other experimental particles or ambient sediment grain size, but apparently increased with experimental particle abundance and time available for feeding. Corophium ingested 9-pm particles at a rate {\textgreater} 20's - I. Predictions from a setal brush model of selection are consistent with these Corophium data. Similarities in mouthpart movements and setal morphology suggest a post-capture sorting mechanism like that of Corophium may exist in a wide variety of crustaceans.},
author = {Miller, Douglas C.},
doi = {10.1016/0022-0981(84)90139-4},
file = {:C$\backslash$:/Users/katelyn.bosley.NMFS/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Miller - 1984 - Mechanical post-capture particle selection by suspension- and deposit-feeding Corophium.pdf:pdf},
